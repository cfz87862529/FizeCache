{
  "name": "fize/cache",
  "description": "一个易于扩展的缓存类库。",
  "license": "MIT",
  "authors": [
    {
      "name": "FizeChan",
      "email": "chenfengzhan@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fize\\Cache\\": "src"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "psr/cache": "^1.0.0",
    "psr/simple-cache": "^1.0.0"
  },
  "suggest": {
    "ext-memcache": "*",
    "ext-memcached": "*",
    "ext-redis": "*",
    "fize/codec": "^5.0.0",
    "fize/database": "^4.0.0",
    "fize/io": "^7.0.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "require-dev": {
    "fize/codec": "^5.0.0",
    "fize/database": "^4.0.0",
    "fize/io": "^7.0.0",
    "phpunit/phpunit": "^9.5.0"
